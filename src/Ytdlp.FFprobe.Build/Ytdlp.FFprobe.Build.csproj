<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFramework>netstandard2.1</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<LangVersion>latest</LangVersion>
	<AssemblyName>Ytdlp.FFprobe.Build</AssemblyName>
	<RootNamespace>Ytdlp.FFprobe.Build</RootNamespace>
	<PackageId>Ytdlp.FFprobe.Build</PackageId>
	<Description>A NuGet package that provides the latest version of `ffprobe.exe`.</Description>
	<Copyright>Copyright © 2026 ManuHub</Copyright>
	<Authors>Manojbabu</Authors>
	<PackageTags>ffprobe;ffprobe.exe;ytdlp;yt-dlp,binary</PackageTags>
	<PackageProjectUrl>https://github.com/yt-dlp/FFmpeg-Builds</PackageProjectUrl>
	<RepositoryUrl>https://github.com/manusoft/Ytdlp-Builds</RepositoryUrl>
	<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
	<Icon>icon.png</Icon>
	<PackageIcon>icon.png</PackageIcon>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<Version>6.2.10</Version>
	<IsPackable>true</IsPackable>
    <DevelopmentDependency>true</DevelopmentDependency>
    <IncludeBuildOutput>false</IncludeBuildOutput>
  </PropertyGroup>

	<ItemGroup>
		<Content Include="icon.png" Pack="true" PackagePath="" />
		<Content Include="README.md" Pack="true" PackagePath="" />
		<Content Include="LICENSE.txt" Pack="true" PackagePath="" />
		<Content Include="Tools\ffprobe.exe" Pack="true" PackageCopyToOutput="true">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

</Project>
